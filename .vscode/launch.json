{
	"configurations": [
		{
			"name": "Launch Program",
			"program": "${workspaceFolder}/app.cjs",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		},
		{
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"name": "Open in Firefox index.html",
			"file": "${workspaceFolder}/index.html"
		}
	]
}