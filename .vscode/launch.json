{
	"configurations": [
		{
			"name": "Launch Node app.cjs",
			"program": "${workspaceFolder}/app.cjs",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		},
		{
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"name": "Open localhost in Firefox",
			"url": "http://localhost/index.html",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"name": "Open as file in Firefox index.html",
			"file": "${workspaceFolder}/index.html"
		}
	]
}